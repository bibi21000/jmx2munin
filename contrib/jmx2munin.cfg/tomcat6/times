graph_title Response Time
graph_vlabel ms
graph_args --base 1000 -l 0
graph_category Tomcat
graph_order count=catalina_globalrequestprocessor_http_8080_requestcount time=catalina_globalrequestprocessor_http_8080_maxtime

catalina_globalrequestprocessor_http_8080_requestcount.label requests
catalina_globalrequestprocessor_http_8080_requestcount.graph no
catalina_globalrequestprocessor_http_8080_requestcount.type DERIVE
catalina_globalrequestprocessor_http_8080_requestcount.min 0

catalina_globalrequestprocessor_http_8080_maxtime.label time
catalina_globalrequestprocessor_http_8080_maxtime.type DERIVE
catalina_globalrequestprocessor_http_8080_maxtime.min 0
catalina_globalrequestprocessor_http_8080_maxtime.graph no

count.graph no
count.label requests
time.graph no
time.label time

rtime.label avg
rtime.info Average response time in milliseconds
rtime.cdef count,0,EQ,0,time,count,/,IF
